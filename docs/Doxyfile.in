PROJECT_NAME           = @PROJECT_NAME@
OUTPUT_DIRECTORY       = @CURRENT_BUILD_DIR@
REPEAT_BRIEF           = NO

BUILTIN_STL_SUPPORT    = YES
INLINE_SIMPLE_STRUCTS  = YES
EXTRACT_PRIV_VIRTUAL   = YES

SHOW_HEADERFILE        = YES
SHOW_INCLUDE_FILES     = NO
INLINE_INFO            = NO

SORT_MEMBER_DOCS       = YES
SORT_BRIEF_DOCS        = YES
SORT_MEMBERS_CTORS_1ST = YES
SORT_BY_SCOPE_NAME     = NO

SHOW_USED_FILES        = NO
SHOW_FILES             = NO
VERBATIM_HEADERS       = NO
STRIP_FROM_INC_PATH    = @PROJECT_SOURCE_ROOT@/include

USE_MDFILE_AS_MAINPAGE = README.md
INPUT                  = @PROJECT_SOURCE_ROOT@/README.md \
                         @PROJECT_SOURCE_ROOT@/include
RECURSIVE              = YES

MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
EXPAND_AS_DEFINED      = RENI_KEY_LIST \
                         RENI_KEY \
                         RENI_MOUSE_BUTTON_LIST \
                         RENI_MOUSE_BUTTON

HAVE_DOT               = @DOT_FOUND@
COLLABORATION_GRAPH    = NO
